@inherits ChartComponentBase


<RadzenChart>
    <RadzenColumnSeries Data="Data" CategoryProperty="@nameof(ChartDataItem.XData)" ValueProperty="@nameof(ChartDataItem.YData)" Title="Something">
        <RadzenMarkers MarkerType="MarkerType.Circle"/>
        <RadzenSeriesDataLabels Visible="false"/>
    </RadzenColumnSeries>
    <RadzenValueAxis Formatter="@FormatAsPercentage">
        <RadzenGridLines Visible="true"/>
    </RadzenValueAxis>
    <RadzenLegend Visible="false"/>
</RadzenChart>