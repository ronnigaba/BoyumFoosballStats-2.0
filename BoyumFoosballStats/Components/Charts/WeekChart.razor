<RadzenChart>
    <RadzenLineSeries Smooth="true" Data="Data" CategoryProperty="Date" Title="Win Rate" ValueProperty="WinRate">
        <RadzenMarkers MarkerType="MarkerType.Circle"/>
        <RadzenSeriesDataLabels Visible="false"/>
    </RadzenLineSeries>
    <RadzenCategoryAxis>
        <RadzenAxisTitle Text="Week number"/>
    </RadzenCategoryAxis>
    <RadzenValueAxis Formatter="@FormatAsPercentage">
        <RadzenGridLines Visible="true"/>
        <RadzenAxisTitle Text="Win Rate (%)"/>
    </RadzenValueAxis>
    <RadzenLegend Visible="false"/>
</RadzenChart>